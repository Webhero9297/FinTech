{
  "name": "reactchartiq",
  "version": "1.0.0",
  "description": "",
  "main": "helloworld.js",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "flux": "^3.1.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "requirejs-babel": "^0.0.9",
    "webpack": "2.2.1"
  },
  "devDependencies": {
    "esformatter-jsx": "^7.2.0",
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0-0",
    "eslint-plugin-standard": "^2.0.1",
    "webpack-dev-server": "^2.4.2"
  },
  "esformatter": {
    "plugins": [
      "esformatter-jsx"
    ],
    "jsx": {
      "formatJSX": true,
      "attrsOnSameLineAsTag": false,
      "maxAttrsOnTag": 3,
      "firstAttributeOnSameLine": true,
      "formatJSXExpressions": true,
      "JSXExpressionsSingleLine": true,
      "alignWithFirstAttribute": false,
      "spaceInJSXExpressionContainers": " ",
      "removeSpaceBeforeClosingJSX": false,
      "closingTagOnNewLine": false,
      "htmlOptions": {}
    }
  },
  "scripts": {
    "build": "webpack --config=webpack.build.config.js",
    "postinstall": "webpack",
    "server": "webpack-dev-server --inline --progress --port 3000 --content-base . --watch=true",
    "start": "webpack-dev-server  --config=webpack.build.config.js --open"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChartIQ/Charting-Library---React-Seed-Project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChartIQ/Charting-Library---React-Seed-Project/issues"
  },
  "homepage": "https://github.com/ChartIQ/Charting-Library---React-Seed-Project#readme"
}
